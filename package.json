{
  "name": "ediphy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/ging/ediphy.git"
  },
  "scripts": {
    "build-production": "cat ediphy && webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build": "cat ediphy && node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "build-dev": "cat ediphy && webpack --config webpack.dev.js",
    "start": "cat ediphy && webpack-dev-server --open --colors --config webpack.dev.js",
    "bundle": "webpack && node bundle_script.js",
    "both": "npm run start-webpack & npm run watch-sass",
    "start-webpack": "webpack-dev-server --progress",
    "watch-sass": "node-sass -w sass/style.scss -o dist/css/compiledsass.css",
    "test": "jest --colors --watch",
    "test:reducers": "jest /reducers/* --colors --watch",
    "test:verbose": "jest --watch --colors --verbose || true",
    "test:coverage": "jest --watch --colors --coverage --verbose || true",
    "test:coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "eslint": "eslint . --fix --ext .js,.jsx,.es6",
    "doc": "cat ediphy && babel-node --presets es2015 doc/scripts/reactdocgenmd.js & node doc/scripts/importComponents.js & webpack --config doc/webpack.doc.prod.js & webpack -p --config webpack.noserver.js & node doc/scripts/bringtodist.js",
    "dev-doc": "cat ediphy && babel-node --presets es2015  doc/scripts/reactdocgenmd.js & node doc/scripts/importComponents.js &  webpack --config webpack.noserver.js & node doc/scripts/bringtodist.js & webpack-dev-server --open --progress --colors --config doc/webpack.doc.dev.js",
    "dev-only-doc": "cat ediphy && webpack-dev-server --open --progress --colors --config doc/webpack.doc.dev.js",
    "add-all": "git add .",
    "create-plugin": "babel-node plugins/create_plugin.js ",
    "create-theme": "babel-node common/themes/create_theme.js "
  },
  "pre-commit": [
    "eslint",
    "add-all"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.*?$": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6",
      "json"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(ejs)$": "<rootDir>/common/__mocks__/ejsMock.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/common/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "<rootDir>/common/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/shim.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testMatch": [
      "**/?(*.)(test|spec).js?(x)"
    ]
  },
  "browserslist": [
    "last 4 version",
    "> 1%"
  ],
