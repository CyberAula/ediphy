{
  "name": "dali_editor",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/ging/DALI_EDITOR.git"
  },
  "scripts": {
    "build-production": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --colors --config webpack.dev.js",
    "bundle": "webpack && node bundle_script.js",
    "both": "npm run start-webpack & npm run watch-sass",
    "start-webpack": "webpack-dev-server --progress",
    "watch-sass": "node-sass -w sass/style.scss -o dist/css/compiledsass.css",
    "test": "jest --colors",
    "eslint": "eslint  --fix  **/*.js **/*.jsx **/*.es6 --ignore-pattern node_modules/ --ignore-pattern dist --ignore-pattern doc/dist --ignore-pattern doc/esdoc --ignore-pattern doc/jsdoc --ignore-pattern sass --ignore-pattern dali_server --ignore-pattern webpack_plugins",
    "doc": " node doc/reactdocgenmd.js & node doc/importComponents.js & webpack --config doc/webpack.doc.prod.js",
    "dev-doc": "node doc/reactdocgenmd.js & node doc/importComponents.js & webpack-dev-server --open --progress --colors --config doc/webpack.doc.dev.js",
    "add-all": "git add .",
    "postinstall": "node ./plugin_install"
  },
  "pre-commit": [
    "eslint",
    "add-all"
  ],
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "testMatch": [
      "**/?(*.)(*test).js"
    ]
  },
  "devDependencies": {
    "async": "^2.1.2",
