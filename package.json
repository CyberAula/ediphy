{
  "name": "ediphy",
  "version": "1.0.0",
  "main": "index.js",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/ging/ediphy.git"
  },
  "scripts": {
    "build-production": "webpack -p --define process.env.NODE_ENV='\"production\"' --progress --colors",
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --colors --config webpack.dev.js",
    "bundle": "webpack && node bundle_script.js",
    "both": "npm run start-webpack & npm run watch-sass",
    "start-webpack": "webpack-dev-server --progress",
    "watch-sass": "node-sass -w sass/style.scss -o dist/css/compiledsass.css",
    "test": "jest --colors",
    "eslint": "eslint . --fix  --ext .js,.jsx,.es6",
    "doc": "babel-node --presets es2015 doc/scripts/reactdocgenmd.js & node doc/scripts/importComponents.js & webpack --config doc/webpack.doc.prod.js & webpack --config webpack.noserver.js & node doc/scripts/bringtodist.js",
    "dev-doc": "babel-node --presets es2015  doc/scripts/reactdocgenmd.js & node doc/scripts/importComponents.js &  webpack --config webpack.noserver.js & node doc/scripts/bringtodist.js & webpack-dev-server --open --progress --colors --config doc/webpack.doc.dev.js",
    "dev-only-doc": "webpack-dev-server --open --progress --colors --config doc/webpack.doc.dev.js",

    "add-all": "git add .",
    "postinstall": "node ./plugin_install"
   },
  "pre-commit": [
    "eslint",
    "add-all"
  ],
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "es6"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(ejs)$": "<rootDir>/core/api.es6"
    },
    "testMatch": [
      "**/?(*.)(*test).js"
    ]
  },
  "devDependencies": {
    "async": "^2.6.0",
    "babel-cli": "^6.26.0",
